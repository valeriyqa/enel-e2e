{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JSON Schema for get_server_info API test",

  "type": "object",
  "properties": {
    "success": {
      "type": "boolean"
    },
    "caller_ip": {
      "type": "string"
    },
    "version_api": {
      "type": "object",
      "properties": {
        "_Major": {
          "type": "integer"
        },
        "_Minor": {
          "type": "integer"
        },
        "_Build": {
          "type": "integer"
        },
        "_Revision": {
          "type": "integer"
        }
      },
      "required": [
        "_Major",
        "_Minor",
        "_Build",
        "_Revision"
      ]
    },
    "version_car_model_db": {
      "type": "string"
    },
    "default_car_model": {
      "type": "object",
      "properties": {
        "model_id": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "year": {
          "type": "integer"
        },
        "range_m": {
          "type": "integer"
        },
        "range_city_m": {
          "type": "integer"
        },
        "range_highway_m": {
          "type": "integer"
        },
        "battery_size_wh": {
          "type": "integer"
        },
        "charging_rate_w": {
          "type": "integer"
        },
        "images": {
          "type": "object",
          "properties": {
            "charging": {
              "type": "string"
            },
            "standby": {
              "type": "string"
            },
            "plugged": {
              "type": "string"
            },
            "error": {
              "type": "string"
            },
            "disconnect": {
              "type": "string"
            }
          },
          "required": [
            "charging",
            "standby",
            "plugged",
            "error",
            "disconnect"
          ]
        }
      },
      "required": [
        "model_id",
        "description",
        "year",
        "range_m",
        "range_city_m",
        "range_highway_m",
        "battery_size_wh",
        "charging_rate_w",
        "images"
      ]
    }
  },
  "required": [
    "success",
    "caller_ip",
    "version_api",
    "version_car_model_db",
    "default_car_model"
  ]
}