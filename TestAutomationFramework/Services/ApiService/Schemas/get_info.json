{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JSON Schema for get_info API test",

  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "zip": {
      "type": "string"
    },
    "country_code": {
      "type": "string"
    },
    "IP": {
      "type": "string"
    },
    "gascost": {
      "type": "integer"
    },
    "mpg": {
      "type": "integer"
    },
    "ecost": {
      "type": "integer"
    },
    "whpermile": {
      "type": "integer"
    },
    "timeZoneId": {
      "type": "string"
    },
    "amps_wire_rating": {
      "type": "integer"
    },
    "amps_unit_rating": {
      "type": "integer"
    },
    "info_timestamp": {
      "type": "integer"
    },
    "garage_id": {
      "type": "string"
    },
    "cars": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "car_id": {
              "type": "integer"
            },
            "description": {
              "type": "string"
            },
            "battery_range_m": {
              "type": "integer"
            },
            "battery_size_wh": {
              "type": "integer"
            },
            "charging_rate_w": {
              "type": "integer"
            },
            "model_id": {
              "type": "string"
            },
            "model_info": {
              "type": "object",
              "properties": {
                "model_id": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "make": {
                  "type": "string"
                },
                "model": {
                  "type": "string"
                },
                "year": {
                  "type": "integer"
                },
                "range_m": {
                  "type": "integer"
                },
                "range_city_m": {
                  "type": "integer"
                },
                "range_highway_m": {
                  "type": "integer"
                },
                "battery_size_wh": {
                  "type": "integer"
                },
                "charging_rate_w": {
                  "type": "integer"
                },
                "images": {
                  "type": "object",
                  "properties": {
                    "default": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "default"
                  ]
                }
              },
              "required": [
                "model_id",
                "description",
                "make",
                "model",
                "year",
                "range_m",
                "range_city_m",
                "range_highway_m",
                "battery_size_wh",
                "charging_rate_w",
                "images"
              ]
            }
          },
          "required": [
            "car_id",
            "description",
            "battery_range_m",
            "battery_size_wh",
            "charging_rate_w",
            "model_id",
            "model_info"
          ]
        }
      ]
    },
    "policy": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "user_control_allowed": {
          "type": "boolean"
        },
        "charge_control_type": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "user_control_allowed",
        "charge_control_type"
      ]
    },
    "unit_type_id": {
      "type": "integer"
    },
    "success": {
      "type": "boolean"
    },
    "ID": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "address",
    "city",
    "zip",
    "country_code",
    "IP",
    "gascost",
    "mpg",
    "ecost",
    "whpermile",
    "timeZoneId",
    "amps_wire_rating",
    "amps_unit_rating",
    "info_timestamp",
    "garage_id",
    "cars",
    "policy",
    "unit_type_id",
    "success",
    "ID"
  ]
}