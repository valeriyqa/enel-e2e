{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JSON Schema for get_state API test",

  "type": "object",
  "properties": {
    "ID": {
      "type": "string"
    },
    "info_timestamp": {
      "type": "integer"
    },
    "show_override": {
      "type": "boolean"
    },
    "state": {
      "type": "string"
    },
    "charging": {
      "type": "object",
      "properties": {
        "amps_limit": {
          "type": "integer"
        },
        "amps_current": {
          "type": "number"
        },
        "voltage": {
          "type": "integer"
        },
        "wh_energy": {
          "type": "integer"
        },
        "savings": {
          "type": "integer"
        },
        "watt_power": {
          "type": "integer"
        },
        "seconds_charging": {
          "type": "integer"
        },
        "wh_energy_at_plugin": {
          "type": "integer"
        },
        "wh_energy_to_add": {
          "type": "integer"
        },
        "flags": {
          "type": "integer"
        },
        "co2": {
          "type": "object",
          "properties": {
            "nonstop": {
              "type": "number"
            },
            "actual": {
              "type": "number"
            }
          },
          "required": [
            "nonstop",
            "actual"
          ]
        }
      },
      "required": [
        "amps_limit",
        "amps_current",
        "voltage",
        "wh_energy",
        "savings",
        "watt_power",
        "seconds_charging",
        "wh_energy_at_plugin",
        "wh_energy_to_add",
        "flags",
        "co2"
      ]
    },
    "lifetime": {
      "type": "object",
      "properties": {
        "wh_energy": {
          "type": "integer"
        },
        "savings": {
          "type": "integer"
        },
        "co2": {
          "type": "object",
          "properties": {
            "nonstop": {
              "type": "number"
            },
            "actual": {
              "type": "number"
            }
          },
          "required": [
            "nonstop",
            "actual"
          ]
        }
      },
      "required": [
        "wh_energy",
        "savings",
        "co2"
      ]
    },
    "charging_time_left": {
      "type": "integer"
    },
    "plug_unplug_time": {
      "type": "integer"
    },
    "target_time": {
      "type": "integer"
    },
    "override_time": {
      "type": "integer"
    },
    "default_target_time": {
      "type": "integer"
    },
    "time_last_ping": {
      "type": "integer"
    },
    "utc_time": {
      "type": "integer"
    },
    "unit_time": {
      "type": "integer"
    },
    "update_interval": {
      "type": "integer"
    },
    "temperature": {
      "type": "integer"
    },
    "frequency": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "triggers_all": {
      "type": "integer"
    },
    "triggers_blocking": {
      "type": "integer"
    },
    "car_id": {
      "type": "integer"
    },
    "success": {
      "type": "boolean"
    }
  },
  "required": [
    "ID",
    "info_timestamp",
    "show_override",
    "state",
    "charging",
    "lifetime",
    "charging_time_left",
    "plug_unplug_time",
    "target_time",
    "override_time",
    "default_target_time",
    "time_last_ping",
    "utc_time",
    "unit_time",
    "update_interval",
    "temperature",
    "frequency",
    "message",
    "triggers_all",
    "triggers_blocking",
    "car_id",
    "success"
  ]
}